#include <stdio.h>

int main() {
    int N;
    int u, d, c, m;

    printf("Ingrese un numero entero positivo menor que 10000: ");
    scanf("%d", &N);

    if (N < 0 || N >= 10000) {
        printf("Error: el numero debe ser positivo y menor que 10000.\n");
        return 1;
    }

    u = N % 10;
    d = (N / 10) % 10;
    c = (N / 100) % 10;
    m = (N / 1000) % 10;

    printf("Millares: %d\n", m);
    printf("Centenas: %d\n", c);
    printf("Decenas:  %d\n", d);
    printf("Unidades: %d\n", u);

    return 0;
}
